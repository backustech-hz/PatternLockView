<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PatternLockView">
        <attr name="patternLockView_dotCount" format="integer"/>
        <attr name="patternLockView_dotNormalSize" format="dimension"/>
        <attr name="patternLockView_dotSelectedSize" format="dimension"/>
        <attr name="patternLockView_pathWidth" format="dimension"/>
        <attr name="patternLockView_aspectRatioEnabled" format="boolean"/>
        <attr name="patternLockView_aspectRatio" format="enum">
            <enum name="square" value="0"/>
            <enum name="width_bias" value="1"/>
            <enum name="height_bias" value="2"/>
        </attr>
        <attr name="patternLockView_normalStateColor" format="color"/>
        <attr name="patternLockView_correctStateColor" format="color"/>
        <attr name="patternLockView_wrongStateColor" format="color"/>
        <attr name="patternLockView_dotAnimationDuration" format="integer"/>
        <attr name="patternLockView_pathEndAnimationDuration" format="integer"/>
    </declare-styleable>
</resources>